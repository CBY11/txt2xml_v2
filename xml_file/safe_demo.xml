<?xml version="1.0" encoding="UTF-8"?>

<g>
    <Factory name="DDG-51">
        <FactoryWaypoints> <!-- 航母、舰艇 途径点 -->
            <WaypointTableEntry> <!-- 途径点 -->
                <Lat>45.0</Lat> <!-- 北纬 -->
                <Lon>-10.0</Lon> <!-- 东经 -->
                <Alt>0</Alt> <!-- 海拔 -->
                <latest/> <!-- 最新目标点 -->
            </WaypointTableEntry>
        </FactoryWaypoints>

        <SomethingLoad> <!-- 武器装载 -->
            <Name>LGM-30G</Name> <!-- 武器名称 -->
            <Count>20</Count> <!-- 装载数量 -->
            <SomethingLoadAttributes> <!-- 武器装载属性 -->
                <UseSystemDefaultSomething>True</UseSystemDefaultSomething>
                <on_land>true</on_land>
                <on_ship>false</on_ship>
                <on_plane>false</on_plane>
                <to_land>true</to_land>
                <to_ship>false</to_ship>
                <to_plane>false</to_plane>
                <range>13000</range>
                <num_warhead>3</num_warhead> <!-- 弹头数量 -->
            </SomethingLoadAttributes>
        </SomethingLoad>

        <SomethingLoad> <!-- 武器装载 -->
            <Name>F-35</Name> <!-- 武器名称 -->
            <Count>10</Count> <!-- 装载数量 -->
            <SomethingLoadAttributes> <!-- 武器装载属性 -->
                <UseSystemDefaultSomething>True</UseSystemDefaultSomething>
                <speed_mach>1.6</speed_mach>
                <stealth>true</stealth>
                <aircraft_gun>true</aircraft_gun>
                <max_height>18300</max_height>
                <missile_load> </missile_load>  <!-- 空列表表示无挂载导弹 -->
            </SomethingLoadAttributes>
        </SomethingLoad>


    </Factory>

